{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "prismjs", {
        "languages": ["javascript", "css", "html", "csharp"],
        "plugins": ["line-numbers"],
        "css": true
      }]
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"],
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": "commonjs",
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
