{
  "name": "lebenslauf-server",
  "description": "Feathers-Server zur Darstellung eines Lebenslaufes",
  "version": "0.0.1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": "Frederik Schmatz",
  "license": "MIT",
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --no-notify src/ --init",
    "start": "npm run compile && node lib/",
    "build": "npm run compile",
    "restart": "npm run compile && node lib/ --init",
    "restart-fake": "npm run compile && node lib/ --init --fake",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "test": "npm run compile && npm run coverage",
    "coverage": "nyc npm run mocha",
    "mocha": "npm run clean && cross-env NODE_ENV=test ts-mocha \"test/**/*.ts\" --recursive --exit",
    "clean": "shx rm -rf test/data/",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.2",
    "@feathersjs/authentication-local": "^4.5.2",
    "@feathersjs/authentication-oauth": "^4.5.2",
    "@feathersjs/configuration": "^4.5.2",
    "@feathersjs/errors": "^4.5.2",
    "@feathersjs/express": "^4.5.2",
    "@feathersjs/feathers": "^4.5.2",
    "@feathersjs/socketio": "^4.5.2",
    "@feathersjs/transport-commons": "^4.5.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dauria": "^2.0.0",
    "feathers-blob": "^2.2.0",
    "feathers-sequelize": "^6.1.0",
    "fs-blob-store": "^5.2.1",
    "helmet": "^3.21.3",
    "multer": "^1.4.2",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "serve-favicon": "^2.5.0",
    "winston": "^3.2.1",
    "yargs": "^15.3.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mocha": "^7.0.2",
    "@types/multer": "^1.4.2",
    "@types/serve-favicon": "^2.5.0",
    "@types/validator": "^12.0.1",
    "@types/webpack-env": "^1.15.1",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^6.2.2",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "shx": "^0.3.2",
    "ts-mocha": "^6.0.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
